<% include partials/header.ejs %>
    <% posts.forEach(function(post){ %>
        <div class="ui container">
            <h3><%=post.title%></h3>
            <img src="<%=post.image%>" >
            <p><%=post.body.substring(0,50)%></p>
            <p><%=post.create%></p>
            <a class="ui black button" href="/posts/<%= post._id%>">Read More</a>
        </div>
   <% }); %>

<% include partials/footer.ejs %>


